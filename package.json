{
    "name": "fe-algorithm",
    "version": "1.0.0",
    "description": "some algorithm questions often asked in frontEnd interviews",
    "repository": "https://github.com/tjx666/fe-algorithm",
    "author": "YuTengjing",
    "license": "MIT",
    "private": true,
    "scripts": {
        "test": "mocha",
        "coverage": "nyc --reporter=lcov yarn test",
        "coverage-upload": "nyc --reporter=lcov yarn test && codecov",
        "test-report": "mocha --reporter mochawesome",
        "lint": "eslint -c .eslintrc.js --ext .ts ./{src,test}/**/*.ts",
        "clean": "rimraf -rf ./.nyc_output ./coverage ./mochawesome-report"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint -c .eslintrc.js"
        ],
        "*.{ts,json,md}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@types/benchmark": "^1.0.31",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.9.2",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "audit-ci": "^2.5.1",
        "benchmark": "^2.1.4",
        "codecov": "^3.7.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-plugin-import": "^2.20.1",
        "espower-typescript": "^9.0.2",
        "husky": "^4.2.3",
        "lint-staged": "^10.0.8",
        "lodash": "^4.17.15",
        "mocha": "^7.1.1",
        "mochawesome": "^5.0.0",
        "nyc": "^15.0.0",
        "power-assert": "^1.6.1",
        "prettier": "^1.19.1",
        "ts-node": "^8.7.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.8.3"
    }
}
